#. module: mission_order
#: report:mission_order:0
msgid "select rp.name as company, rp.street, rp.city, rp.zip, rco.name as country, rc.phone, rc.email, rp.fax, rp.vat, mo.name mission, mo.date_start, mo.date_end, he.name_related as employee, fv.name, mo.route, '[' || pp.default_code || '] ' || pp.name_template as product, sum(sk.qty) as Qty from mission_order mo inner join stock_location sl on sl.id = mo.location_id inner join hr_employee he on he.id = mo.employee_id inner join fleet_vehicle fv on fv.id = mo.vehicle_id inner join stock_quant sk on sk.location_id = sl.id inner join product_product pp on pp.id = sk.product_id inner join res_company rc on rc.id = sk.company_id inner join res_partner rp on rp.id = rc.partner_id left outer join res_country rco on rco.id = rp.country_id where mo.id=$P{mission_id} group by company, street, city, zip, country, rc.phone, rc.email, fax, vat, mo.name, mo.date_start, mo.date_end, he.name_related, fv.name, mo.route, product order by product"
msgstr "select rp.name as company, rp.street, rp.city, rp.zip, rco.name as country, rc.phone, rc.email, rp.fax, rp.vat, mo.name mission, mo.date_start, mo.date_end, he.name_related as employee, fv.name, mo.route, '[' || pp.default_code || '] ' || pp.name_template as product, sum(sk.qty) as Qty from mission_order mo inner join stock_location sl on sl.id = mo.location_id inner join hr_employee he on he.id = mo.employee_id inner join fleet_vehicle fv on fv.id = mo.vehicle_id inner join stock_quant sk on sk.location_id = sl.id inner join product_product pp on pp.id = sk.product_id inner join res_company rc on rc.id = sk.company_id inner join res_partner rp on rp.id = rc.partner_id left outer join res_country rco on rco.id = rp.country_id where mo.id=$P{mission_id} group by company, street, city, zip, country, rc.phone, rc.email, fax, vat, mo.name, mo.date_start, mo.date_end, he.name_related, fv.name, mo.route, product order by product"

