#. module: customer_statement_by_user
#: report:statement_by_user:0
msgid "select ai.id, res_user.name as user, case when ai.type = 'out_invoice' then 'Facture' else 'Avoir' end as type, rp.name partner, ai.date_invoice, ai.number, ai.origin, ai.residual, ai.amount_total from account_invoice ai inner join res_partner rp on rp.id = ai.partner_id inner join res_users ru on ru.id = ai.user_id inner join res_partner as res_user on res_user.id = ru.partner_id where ai.residual != 0 and ai.user_id = $P{user_id} order by ai.partner_id"
msgstr "select ai.id, res_user.name as user, case when ai.type = 'out_invoice' then 'Facture' else 'Avoir' end as type, rp.name partner, ai.date_invoice, ai.number, ai.origin, ai.residual, ai.amount_total from account_invoice ai inner join res_partner rp on rp.id = ai.partner_id inner join res_users ru on ru.id = ai.user_id inner join res_partner as res_user on res_user.id = ru.partner_id where ai.residual != 0 and ai.user_id = $P{user_id} order by ai.partner_id"

