#. module: treasury
#: report:vesement:0
msgid "select av.name, rpb.acc_number rib, rc.name partner, rp.phone, rp.fax, av.date_vesement, av.amount_in_word, rb.name as bank, at.name chq, at.holder, at.amount from account_vesement av inner join res_company rc on rc.id = av.company_id inner join res_partner rp on rp.id = rc.partner_id inner join res_partner_bank rpb on rpb.id = av.bank_target inner join account_vesement_treasury_rel rel on rel.vesement_id = av.id inner join account_treasury at on at.id = rel.treasury_id inner join res_bank rb on rb.id = at.bank_source where av.id = $P{vesement_id} order by at.clearing_date"
msgstr "select av.name, rpb.acc_number rib, rc.name partner, rp.phone, rp.fax, av.date_vesement, av.amount_in_word, rb.name as bank, at.name chq, at.holder, at.amount from account_vesement av inner join res_company rc on rc.id = av.company_id inner join res_partner rp on rp.id = rc.partner_id inner join res_partner_bank rpb on rpb.id = av.bank_target inner join account_vesement_treasury_rel rel on rel.vesement_id = av.id inner join account_treasury at on at.id = rel.treasury_id inner join res_bank rb on rb.id = at.bank_source where av.id = $P{vesement_id} order by at.clearing_date"

#. module: treasury
#: report:cash_vesement:0
msgid "select av.name, rpb.bank_name, rpb.acc_number rib, rc.name partner, rp.phone, rp.fax, av.date_vesement, av.amount, av.amount_in_word from account_vesement_cash av inner join res_company rc on rc.id = av.company_id inner join res_partner rp on rp.id = rc.partner_id inner join res_partner_bank rpb on rpb.id = av.bank_target where av.id = $P{vesement_id}"
msgstr "select av.name, rpb.bank_name, rpb.acc_number rib, rc.name partner, rp.phone, rp.fax, av.date_vesement, av.amount, av.amount_in_word from account_vesement_cash av inner join res_company rc on rc.id = av.company_id inner join res_partner rp on rp.id = rc.partner_id inner join res_partner_bank rpb on rpb.id = av.bank_target where av.id = $P{vesement_id}"

