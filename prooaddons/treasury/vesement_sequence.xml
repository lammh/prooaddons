<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for vesement -->
        <record id="seq_type_vesement" model="ir.sequence.type">
            <field name="name">Vesement</field>
            <field name="code">account.vesement</field>
        </record>

        <record id="seq_vesement" model="ir.sequence">
            <field name="name">Vesement</field>
            <field name="code">account.vesement</field>
            <field name="prefix">V</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for cash vesement -->
        <record id="seq_type_vesement_cash" model="ir.sequence.type">
            <field name="name">Cash Vesement</field>
            <field name="code">account.vesement.cash</field>
        </record>

        <record id="seq_vesement_cash" model="ir.sequence">
            <field name="name">Cash Vesement</field>
            <field name="code">account.vesement.cash</field>
            <field name="prefix">V</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for cash withdrawal -->
        <record id="seq_type_withdrawal_cash" model="ir.sequence.type">
            <field name="name">Cash Withdrawal</field>
            <field name="code">account.withdrawal</field>
        </record>

        <record id="seq_withdrawal_cash" model="ir.sequence">
            <field name="name">Cash Withdrawal</field>
            <field name="code">account.withdrawal</field>
            <field name="prefix">R</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
