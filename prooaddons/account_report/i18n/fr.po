#. module: withholding_report
#: report:account_voucher1:0
msgid "select  sum(vl.amount_original)as original, sum(vl.amount_original* at.amount) as witholding, rp.name as partner,rp.street,current_date as dt, at.amount*100 as t from account_voucher av inner join account_voucher_line vl on vl.voucher_id=av.id inner join account_tax at on av.withholding = at.id inner join res_partner rp on av.partner_id = rp.id where av.id = $P{voucher_id} group by rp.vat,rp.name,rp.street,t"
msgstr "select  sum(vl.amount_original)as original, sum(vl.amount_original* at.amount) as witholding, rp.name as partner,rp.street,current_date as dt, at.amount*100 as t from account_voucher av inner join account_voucher_line vl on vl.voucher_id=av.id inner join account_tax at on av.withholding = at.id inner join res_partner rp on av.partner_id = rp.id where av.id = $P{voucher_id} group by rp.vat,rp.name,rp.street,t"

